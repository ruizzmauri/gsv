{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "gsv-channel-test",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  // Service binding to Gateway for RPC
  "services": [
    {
      "binding": "GATEWAY",
      "service": "gateway",
      "entrypoint": "GatewayEntrypoint"
    }
  ]
}
